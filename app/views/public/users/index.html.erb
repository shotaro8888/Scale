<div class="container">
  <div class="row">
    <% @users.each do |user| %>
    <div class="card" style="width: 18rem;">
      <%= image_tag user.get_profile_image(300,300), class: "img-fluid" %>
      <div class="card-body">
        <p class="card-text"><%= user.last_name %><%= user.first_name %></p>
      </div>
    </div>
    
    <% end %>
    
    
  </div>
</div>
