<div class="container">
  <div class="row mt-3">
    <div class="col-4"></div>
    <div class="col-4">
      <table class="table table-bordered table-dark">
        <thead>
          <tr><th>ユーザ一覧</th></tr>
        </thead>
        <% @users.each do |user| %>
        <tbody>
          <tr>
            <td><%= link_to user_path(user.id) do %><%= user.last_name %><%= user.first_name %><% end %></td>
          </tr>
        </tbody>
        <% end %>
      </table>
    </div>
    <div class="col-4"></div>
  </div>
</div>
