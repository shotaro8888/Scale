<div class="container">
  <div class="row">
    <%= form_with model: @genre, url: admin_genres_path, method: :post do |f| %>
      <%= f.label :name, "魚種ジャンル" %>
      <%= f.text_field :name %>
      <%= f.submit '新規登録' %>
    <% end %>
  </div>
  <div class="row">
    <% @genres.each do |genre| %>
    <%= genre.name %>
    <%= link_to "編集する", edit_admin_genre_path(genre.id) %>
    <%= link_to "削除する", admin_genre_path(genre.id), method: :delete %>
    <% end %>
  </div>
</div>