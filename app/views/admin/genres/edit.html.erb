<div class="container">
  <div class="row mt-3">
    <div class="col-offset-2 col-lg-8 mx-auto">
      <%= form_with model: @genre, url: admin_genres_path, method: :post do |f| %>
        <%= f.label :name, "魚種ジャンル" %>
        <%= f.text_field :name %>
        <%= f.submit '変更を保存', class: 'btn btn-md btn-success' %>
      <% end %>
    </div>
  </div>
</div>