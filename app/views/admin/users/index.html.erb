<div class="container">
  <div class="row mt-3">
    <div class="col-offset-2 col-lg-8 mx-auto">
    <table class="table table-striped table-dark">
      <thead>
        <tr>
          <th>会員ID</th>
          <th>ユーザ名</th>
          <th>ユーザ画像</th>
          <th></th>
          <th></th>
        </tr>
      </thead>
      <% @users.each do |user| %>
      <tbody>
        <tr>
          <td><%= user.id %></td>
          <td><%= link_to admin_user_path(user.id) do %><%= user.last_name %><%= user.first_name %><% end %></td>
          <td><%= image_tag user.get_profile_image(100,100), class: "img-fluid" %></td>
          <td><%= user.is_deleted ? '退会' : '有効' %></td>
          <td><%= link_to "編集する", edit_admin_user_path(user.id), class: 'btn btn-md btn-info' %></td>
        </tr>
      </tbody>
      <% end %>
    </table>
    
    
    
  </div>
</div>